{
  "name": "atk4/login",
  "type": "library",
  "description": "Login and User module for Agile UI",
  "keywords": [
    "user",
    "acl",
    "auth",
    "login",
    "atk4",
    "agile",
    "agile ui",
    "data",
    "framework"
  ],
  "homepage": "https://github.com/atk4/login",
  "license": "MIT",
  "authors": [
    {
      "name": "Romans Malinovskis",
      "email": "romans@agiletoolkit.org",
      "homepage": "https://nearly.guru/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.3.0",
    "atk4/ui": "~2.3.0",
    "atk4/data": "~2.3.0"
  },
  "require-dev": {
    "atk4/schema": "~2.3.0",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpunit": "*",
    "symfony/contracts": ">=1.1"
  },
  "autoload": {
    "psr-4": {
      "atk4\\login\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "atk4\\login\\tests\\": "tests/"
    }
  }
}
